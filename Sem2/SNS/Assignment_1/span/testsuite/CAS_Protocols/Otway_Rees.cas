protocol OtwayRees;
identifiers
A, B, S 	: user;
M, Na, Nb 	: number;
Kas, Kbs, Kab 	: symmetric_key;

messages
1. A -> B 	: M, A, B, {Na, M, A, B}Kas
2. B -> S 	: M, A, B, {Na, M, A, B}Kas , {Nb, M, A, B}Kbs
3. S -> B 	: M, {Na, Kab}Kas, {Nb, Kab}Kbs
4. B -> A 	: M, {Na, Kab}Kas

knowledge
A	: A,B,S,Kas;
B	: A,B,S,Kbs;
S	: A,B,S,Kas,Kbs;

session_instances
 [A:alice,B:bob,S:server,Kas:key1,Kbs:key2];

goal
 secrecy_of Kab [];