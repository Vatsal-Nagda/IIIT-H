protocol NeedhamSchroederPublicKey;
identifiers
A,B		: user;
Na,Nb 		: number;
KPa,KPb 	: public_key;

messages
1. A -> B 	: {Na, A}KPb
2. B -> A 	: {Na, Nb}KPa
3. A -> B 	: {Nb}KPb

knowledge
A	: A,B,KPa,KPb;
B	: A,B,KPa,KPb;

session_instances
 [A:alice,B:bob,KPa:ka,KbP:kb];

goal
 A authenticates B on Na;

